<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Document</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" />
  <style>
    .previous {
      text-decoration: none;
      display: inline-block;
      padding: 8px 16px;
      background-color: #f1f1f1;
      margin-bottom: 15px;
    }
  </style>
</head>

<body class="container" style="margin-top: 50px">
  <div>
    <a href="#" class="previous" type="button" onclick="history.back(-1)">&#8249; Previous</a>
  </div>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">STT</th>
        <th scope="col">Name table</th>
      </tr>
    </thead>
    <tbody>
      <% for(var i=0; i < data.length; i++) { %>
        <tr>
          <th scope="row">
            <%= (i + 1)%>
          </th>
          <td>
            <%= data[i]%>
          </td>
          <td>
            <a class="btn btn-success edit" href="../table?table=<%= data[i]%>">Show</a>
          </td>
        </tr>
        <% } %>
    </tbody>
  </table>
</body>

</html>